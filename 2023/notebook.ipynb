    "# Day 1\n",
    "\n",
    "input = open('inputs/1.txt', 'r').read().split('\\n')\n",
    "output = 0\n",
    "\n",
    "for line in input:\n",
    "    digits = [char for char in line if char.isdigit()]\n",
    "    output += int(digits[0] + digits[-1])\n",
    "    \n",
    "print(output)"
   ]
  }
